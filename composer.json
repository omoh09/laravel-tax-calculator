{
    "name": "dr-omoh/tax-calculator",
    "description": "A Laravel package to calculate taxes for multiple countries",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "DrOmoh\\TaxCalculator\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Gabriel",
            "email": "gabrielifoga@yahoo.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "illuminate/support": "^9.0|^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "DrOmoh\\TaxCalculator\\TaxCalculatorServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "squizlabs/php_codesniffer": "^3.10",
        "phpstan/phpstan": "^1.11"
    }
}
